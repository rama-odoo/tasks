<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="web_inherit_template_website_contactus" inherit_id="website.contactus">
        <xpath expr="//div[hasclass('form-group','col-12','s_website_form_field','s_website_form_custom')]" position="after">

            <div class="form-group col-12 s_website_form_field s_website_form_contactus" data-type="char" data-name="Field">
                <div class="row s_col_no_resize s_col_no_bgcolor">
                    <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact6">
                        <span class="s_website_form_label_content">City</span>
                    </label>
                    <div class="col-sm">
                        <input id="contact6" type="text" class="form-control s_website_form_input" name="city" />
                    </div>
                </div>
            </div>

        </xpath>
    </template>

</odoo>