<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_quant_tree_inventory_editable_inherit_view" model="ir.ui.view">
        <field name="name">stock.quant.tree.inventory.editable.inherit.view</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='lot_id']" position="after">
                <field name="x_device_imei_Number"/>
                <field name="x_sim1"/>
                <field name="x_sim2" optional="hide"/>
            </xpath>
        </field>
    </record>
</odoo>
